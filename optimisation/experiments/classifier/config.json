{
    "language"        : "PYTHON",
    "main-file"       : "optimise.py",
    "experiment-name" : "optimisation-classifier",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "local",
            "max-concurrent"    : 7,
            "max-finished-jobs" : 100
        }
    },
    "likelihood"      : "GAUSSIAN",
    "variables" : {
        "classifier/compile/lr" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 1E-05,
            "max"  : 1E-02
        },
        "classifier/compile/decay" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 1E-05,
            "max"  : 1E-02
        },
        "classifier/model/default/units" : {
            "type" : "INT",
            "size" : 1,
            "min"  : 1,
            "max"  : 512
        },
        "classifier/model/default/dropout" : {
            "type" : "float",
            "size" : 1,
            "min"  : 0,
            "max"  : 1
        },
        "classifier/model/default/activation" : {
            "type" : "ENUM",
            "size" : 1,
            "options" : ["tanh", "relu"]
        }
    }
}
